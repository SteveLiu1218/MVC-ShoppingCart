@model List<ShoppingCartPractice.Models.ProductLists>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>商品列表</h2>
<table class="table">
    <thead>
        <tr>
            <td>
                編號
            </td>
            <td>
                名稱
            </td>
            <td>
                售價
            </td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.Name
            </td>
            <td>
                @item.Price
            </td>
        </tr>
        }
    </tbody>
</table>
